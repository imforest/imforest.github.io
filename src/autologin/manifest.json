{
  "manifest_version": 2,

  "name": "AutologinForDev",
  "description": "My lovely autologin for developers",
  "version": "1.0",
 
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Help login",
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["https://*.google.com/*"]
    }
  ],

  "permissions": [
    "activeTab",
    "storage","tabs",
    "<all_urls>"
  ]
}